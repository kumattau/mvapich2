# A special testing target to simplify running just the datatype tests
testing:
	../runtests -srcdir=$(srcdir) -tests=testlist \
		   -mpiexec=@MPIEXEC@ \
		   -xmlfile=summary.xml

clean-local:
	-rm -f summary.xml
