all: all-redirect

SHELL           = @SHELL@
bindir          = @bindir@
exec_prefix     = @exec_prefix@
prefix          = @prefix@
abs_builddir    = @abs_builddir@
srcdir          = @srcdir@
abs_srcdir      = @abs_srcdir@
MAKE            = @MAKE@
MPILIBNAME      = @MPILIBNAME@
PMPILIBNAME     = @PMPILIBNAME@


VPATH = .:@srcdir@

.SUFFIXES:
# Some make programs complain if no suffixes are set
.SUFFIXES: .c .o
apply:
	$(ACTION) $(ACTION_INPUT)
	(cd pt2pt && $(MAKE) apply )
clean:
	-@rm -f *.o ${srcdir}/*.o 
	-@rm -f *.lo ${srcdir}/*.lo
	(cd pt2pt && $(MAKE) clean )
distclean: clean distclean-xxx remove-makefile
distclean-xxx:
	(cd pt2pt && $(MAKE) distclean )
	-@rm -f Makefile
	-@rm -rf autom4te*.cache
	(cd f77 && $(MAKE) distclean )
	(cd cxx && $(MAKE) distclean )
	-@rm -rf $(DEPS_DIR)
	-@rm -f TAGS
remove-makefile:
	rm -f Makefile
remove-genmakefiles:
	rm -f  Makefile
all-redirect:
	@(cd pt2pt && ${MAKE} all )

# --------------------------------------------------------------------------
# --------------------------------------------------------------------------
# --------------------------------------------------------------------------
HEADERFILES     = $(HEADERS)
SOURCEFILES     = $(SOURCES)
tags: TAGS
TAGS:
	(cd pt2pt && $(MAKE) TAGS )
# --------------------------------------------------------------------------
#SUBDIRS = util attr basic datatype coll comm errhan group info init pt2pt rma spawn topo io @otherlangs@ .

# A special testing target to simplify running just the thread tests
testing:
	../runtests -srcdir=$(srcdir) -tests=testlist \
		   -mpiexec=$(bindir)/mpiexec \
		   -xmlfile=summary.xml

FORCE_TARGET:

