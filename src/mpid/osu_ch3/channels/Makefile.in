# This Makefile.in created by simplemake.  Do not edit

# @configure_input@
all: all-redirect

SHELL           = @SHELL@
srcdir          = @srcdir@
MAKE            = @MAKE@
MPILIBNAME      = @MPILIBNAME@


VPATH = .:${srcdir}
.SUFFIXES:
.SUFFIXES: .o .lo .c 
clean:
	-@rm -f *.o ${srcdir}/*.o 
	-@rm -f *.lo ${srcdir}/*.lo
	for dir in @channel_name@ - ; do \
		if [ "$$dir" = "-" ] ; then break ; fi ; \
		(cd $$dir && ${MAKE} clean ; ) ; done
distclean: clean distclean-xxx remove-makefile
distclean-xxx:
	for dir in @channel_name@ - ; do \
		if [ "$$dir" = "-" ] ; then break ; fi ; \
		(cd $$dir && ${MAKE} distclean ; ) ; done
	-@rm -f Makefile
	-@rm -rf autom4te*.cache
	-@for dir in  ib shm sock ssm rdma gasnet sshm ; do \
	    if [ -s $$dir/Makefile ] ; then \
	      (cd $$dir && ${MAKE} distclean ;) ; fi ; \
	done
	-@rm -rf $(DEPS_DIR)
	-@rm -f TAGS
remove-makefile:
	rm -f Makefile
remove-genmakefiles:
	rm -f  Makefile

all-redirect:
	@for dir in @channel_name@ - ; do \
		if [ "$$dir" = "-" ] ; then break ; fi ; \
		if ( cd $$dir && ${MAKE} all ) ; then : ; else exit 1; fi \
	done

tags: TAGS
TAGS:
	for dir in @channel_name@ - ; do \
		if [ "$$dir" = "-" ] ; then break ; fi ; \
		(cd $$dir && ${MAKE} TAGS ; ) ; done
# SUBDIRS_channel_name contains the list of all possible channel
# implementations


FORCE_TARGET:

