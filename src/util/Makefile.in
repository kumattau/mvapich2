all: all-redirect

SHELL           = @SHELL@
abs_builddir    = @abs_builddir@
srcdir          = @srcdir@
abs_srcdir      = @abs_srcdir@
MAKE            = @MAKE@
MPILIBNAME      = @MPILIBNAME@
PMPILIBNAME     = @PMPILIBNAME@


VPATH = .:@srcdir@

.SUFFIXES:
# Some make programs complain if no suffixes are set
.SUFFIXES: .c .o
apply: 
	$(ACTION) $(ACTION_INPUT)
	for dir in mem info msgs instrm thread dbg param osserv @logging_dir@ - ; do \
		if [ "$$dir" = "-" ] ; then break ; fi ; \
		(cd $$dir && ${MAKE} apply ; ) ; done
clean: 
	-@rm -f *.o ${srcdir}/*.o 
	-@rm -f *.lo ${srcdir}/*.lo
	for dir in mem info msgs instrm thread dbg param osserv @logging_dir@ - ; do \
		if [ "$$dir" = "-" ] ; then break ; fi ; \
		(cd $$dir && ${MAKE} clean ; ) ; done
distclean:  clean distclean-xxx remove-makefile
distclean-xxx:
	for dir in mem info msgs instrm thread dbg param osserv @logging_dir@ - ; do \
		if [ "$$dir" = "-" ] ; then break ; fi ; \
		if [ ! -s $$dir/Makefile ] ; then continue ; fi ;\
		(cd $$dir && ${MAKE} distclean ; ) ; done
	-@rm -f createshlib Makefile
	-@rm -rf autom4te*.cache
	if [ -s logging/Makefile ] ; then (cd logging && $(MAKE) distclean ) ; fi
	-@rm -rf $(DEPS_DIR)
	-@rm -f TAGS
remove-makefile:
	rm -f Makefile
remove-genmakefiles:
	rm -f  Makefile
all-redirect:
	@(cd mem && ${MAKE} all )
	@(cd info && ${MAKE} all )
	@(cd msgs && ${MAKE} all )
	@(cd instrm && ${MAKE} all )
	@(cd thread && ${MAKE} all )
	@(cd dbg && ${MAKE} all )
	@(cd param && ${MAKE} all )
	@(cd osserv && ${MAKE} all )
	@for dir in @logging_dir@ - ; do \
		if [ "$$dir" = "-" ] ; then break ; fi ; \
		if ( cd $$dir && ${MAKE} all ) ; then : ; else exit 1; fi \
	done

# --------------------------------------------------------------------------
# --------------------------------------------------------------------------
# --------------------------------------------------------------------------
HEADERFILES     = $(HEADERS)
SOURCEFILES     = $(SOURCES)
tags: TAGS
TAGS: Makefile
	for dir in mem info msgs instrm thread dbg param osserv @logging_dir@ - ; do \
		if [ "$$dir" = "-" ] ; then break ; fi ; \
		(cd $$dir && ${MAKE} TAGS ; ) ; done
