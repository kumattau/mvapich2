top_srcdir      = @top_srcdir@
DEFS            = @DEFS@ -I. -I${srcdir}
CPPFLAGS        = @CPPFLAGS@
LIBS            = @EXAMPLE_LIBS@@LIBS@
MAKE            = @MAKE@
MPILIBNAME      = @MPILIBNAME@
